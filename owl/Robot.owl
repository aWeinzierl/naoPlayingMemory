        <AnnotationProperty abbreviatedIRI="rdfs:comment"/>
        <IRI>#Me</IRI>
        <Literal datatypeIRI="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral">Associate won Card pairs to &quot;Me&quot;</Literal>
    </AnnotationAssertion>
    <AnnotationAssertion>
